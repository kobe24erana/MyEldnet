@model List<MyMvcApplication.Models.EnrollmentHeader>
<html>
<head>
    <title>EnrollmentHeader</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
    <link rel="stylesheet" href="~/css/Student/StudentIndex.css" asp-append-version="true" />
</head>
<body>

    <h2>EnrollmentHeader List</h2>
    <div class="table-wrapper">
        <div class="container text-center">
            <div class="row row-cols-auto">                         
            </div>
        </div>
        <table class="fl-table">
            <thead>
                <tr class="table-dark">
                    <th scope="col">ID</th>
                    <th scope="col">Date Enroll</th>
                    <th scope="col">School Year</th>
                    <th scope="col">Encoder</th>
                    <th scope="col">Total Units</th>
                    <th scope="col">Status</th>              
                </tr>
            </thead>
            <tbody>
                @foreach (var EnrollmentHeader in Model)
                {
                    <tr>
                        <td>@EnrollmentHeader.ENRHSTUDID</td>
                        <td>@EnrollmentHeader.ENRHSTUDDATEENROLL</td>
                        <td>@EnrollmentHeader.ENRHSTUDSCHLYR</td>
                        <td>@EnrollmentHeader.ENRHSTUDENCODER</td>
                        <td>@EnrollmentHeader.ENRHSTUDTOTALUNITS</td>
                        <td>@EnrollmentHeader.ENRHSTUDSTATUS</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>

